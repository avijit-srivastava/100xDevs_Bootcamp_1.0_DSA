import java.util.Scanner;
 
public class test {
    public static void main(String[] args) {
      
        Scanner in = new Scanner(System.in);
        int a = in.nextInt();
        int b = in.nextInt();
 
        if(a <= b) {
            System.out.println("Min = "  + a);
            System.out.println("Max = " + b);
        }
        else if(b <= a) {
            System.out.println("Min = " + b);
            System.out.println("Max = " + a);
        }
        
    }
}
