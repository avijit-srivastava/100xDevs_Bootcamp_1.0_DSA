import java.util.Scanner;
 
public class test {
    public static void main(String[] args) {
      
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        int m = in.nextInt();
        
        if((m % n) == 0) {
            System.out.println("Yes");
        }
        else {
            System.out.println("No");
        }
    }
}
