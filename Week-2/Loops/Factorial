import java.util.Scanner;

public class test {
    public static void main(String[] args) {
        try (Scanner in = new Scanner(System.in)) {
           int n = in.nextInt();
           int ans = solve(n);
           System.out.println(ans);
        }
    }
    public static int solve(int n) {
        int prod = 1;
        for(int i=2; i<=n; i++) {
            prod = prod * i;
        }
        return prod;
    }
}
