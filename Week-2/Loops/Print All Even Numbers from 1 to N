import java.util.Scanner;

public class test {
    public static void main(String[] args) {
        try (Scanner in = new Scanner(System.in)) {
            int n = in.nextInt();
            solve(n);
        }
    }
    public static void solve(int n) {
        for(int i=1; i<=n; i++) {
            if(i%2 == 0) {
                System.out.print(i + " ");
            }
        }
    }
}
