import java.util.Scanner;

public class test {
    public static void main(String[] args) {
        try (Scanner in = new Scanner(System.in)) {
           long n = in.nextLong();
           System.out.println(solve(n));
        }
    }
    public static long solve(long n) {
        long sum = 0;
        while(n != 0) {
            int digit = (int)(n % 10);
            sum = sum + digit;
            n = n / 10;
        }
        return sum;
    }
}
